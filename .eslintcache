apped ui={false}/>\n            <Card.Content>\n              <Card.Header>{user.name}</Card.Header>\n              <Card.Meta>\n                <span className='date'>{user.email}</span>\n              </Card.Meta>\n              <Card.Description>\n                {user.address.city}, {user.address.street} {user.address.zipcode}\n              </Card.Description>\n            </Card.Content>\n            <Card.Content extra>\n              {user.company.name}\n            </Card.Content>\n            <Card.Content as=\"h3\" extra>\n              <Link style={{color:\"red\"}} to={`${url}/posts`}>Show posts</Link>\n            </Card.Content>\n            <Card.Content as=\"h3\" extra>\n              <Link style={{color:\"green\"}} to={`${url}/todos`}>Show TODOs</Link>\n            </Card.Content>\n            <Card.Content extra>\n              <Icon name='camera'/>\n              {albums.length} Albums\n              <List>\n                {albums.map(album => <List.Item>\n                  <Link to={`${url}/albums/${album.id}`}>{album.title}</Link>\n                </List.Item> )}\n              </List>\n            </Card.Content>\n          </Card>\n        </Grid.Column>\n        <Grid.Column width={10}>\n          <Switch>\n            <Route exact path={path}>\n              <Header as='h3'>Select an album</Header>\n            </Route>\n            <Route path={`${path}/albums/:albumId`}>\n              <Album />\n            </Route>\n            <Route path={`${path}/posts`}>\n              <UserPosts userId={user.id} />\n            </Route>\n            <Route path={`${path}/todos`}>\n              <UserTodos userId={user.id} />\n            </Route>\n            <Route path='*'>\n              <Header as='h3'>Select an album</Header>\n            </Route>\n          </Switch>\n        </Grid.Column>\n      </Grid>\n      }\n    </Container>\n  );\n}\n\nexport default UserDetails;\n","D:\\Projects\\homework\\hw1-vh-react\\src\\blog-v2\\containers\\NotF